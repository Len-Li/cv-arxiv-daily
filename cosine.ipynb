    "\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "def tanh(x):\n",
    "    s1 = np.exp(x) - np.exp(-x)\n",
    "    s2 = np.exp(x) + np.exp(-x)\n",
    "    s = s1 / s2\n",
    "    return s\n",
    "\n",
    "\n",
    "x = np.linspace(-5, 5, num=256)\n",
    "\n",
    "y = tanh(x)\n",
    "\n",
    "plt.plot(x, y)\n",
    "plt.show()\n"
   ]
  }
